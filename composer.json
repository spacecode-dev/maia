{
    "name": "spacecode-dev/maia",
    "description": "Admin panel handling for Laravel based on Laravel Nova",
    "keywords": [
        "maia",
        "laravel",
        "admin",
        "panel",
        "dashboard",
        "cms"
    ],
    "homepage": "https://github.com/spacecode-dev/maia",
    "license": "MIT",
    "authors": [
        {
            "name": "Vladlen Beilik",
            "email": "vladlen.beilik@gmail.com",
            "homepage": "https://spacecode.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "spatie/laravel-permission": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "SpaceCode\\Maia\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SpaceCode\\Maia\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}