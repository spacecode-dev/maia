{
    "name": "spacecode-dev/maia",
    "description": "Admin panel handling for Laravel based on Laravel Nova",
    "keywords": [
        "maia",
        "laravel",
        "admin",
        "panel",
        "dashboard",
        "cms"
    ],
    "homepage": "https://github.com/spacecode-dev/maia",
    "license": "MIT",
    "authors": [
        {
            "name": "Vladlen Beilik",
            "email": "vladlen.beilik@gmail.com",
            "homepage": "https://spacecode.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "5.6.*|5.7.*|5.8.*|6.*",
        "illuminate/http": "5.6.*|5.7.*|5.8.*|6.*",
        "illuminate/auth": "^5.8|^6.0",
        "illuminate/container": "^5.8|^6.0",
        "illuminate/contracts": "^5.8|^6.0",
        "illuminate/database": "^5.8|^6.0",
        "intervention/image": "^2.4",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^4.0",
        "phpunit/phpunit": "^8.0",
        "predis/predis": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "SpaceCode\\Maia\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SpaceCode\\Maia\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
